.subroutine_clock1_period event
.local variable result, text
.local variable command, text

command = "adb shell getprop ro.product.model"
result = system_get DOS execution result (command)

.if (result = "adb.exe: no devices/emulators found" + #newline)
    Transparent Label 1. Title = "Device Not Connected"
    Transparent Label 2. Title = "No connection to your device found, please make sure to enable ADB debugging"
    Group Box 1. Left = 76
    Picture frame 38. Picture = #picture1
    Picture Frame 39. Background Color = #Silver White

    Picture frame 42. Picture = #picture4
    Picture Box 43. Background Color = Picture Box 44. Background Color
    Picture frame 40. Picture = #picture4
    Picture Box 41. Background Color = Picture Box 44. Background Color


    GroupBox1.TopEdge = 195

.otherwise
    transparent label 1. title = result
    Group Box 1. Left = 2000
    GroupBox1.TopEdge = 195

    Transparent label 2. Title = "Please keep the watch open ADB debugging, close other programs to maintain stability"
.if end



.' if (result = "Mi Watch" + #newline)
    ' Transparent Label 1. Title = "Xiaomi Watch"
.otherwise

.if end


