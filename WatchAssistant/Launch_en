.subroutine_clock2_period event
.local variable result, text
.local variable command, text
.local variable memory amount, double precision decimal
.local variable random, integer
.local variable rest, integer
.local variable other, integer


command = "adb shell cat /proc/meminfo |findstr MemTotal"
result = system_get DOS execution result (command)


Amount of memory = to decimal(text_getintermediatetext(result, " ", "k", , )) ÷ 1024



transparent label 45. title = to text (rounded up (amount of memory, )) + "MB"

remaining = rounded (memory amount, )

set random number seed ()
random = take a random number (400, 500)



Transparent Label 42. Title = To Text (Remainder - Random) + "MB"

other = remaining - random

Transparent Label 48. Title = To Text (Round (Other ÷ Remainder × 100, )) + "%"

Transparent Label 49. Title = To Text (Remainder - Other) + "MB"
.if (memory_amount = 0)
    Transparent Label 42. Title = "Unavailable"
    Transparent Label 45. Title = "Unavailable"
    Transparent Label 48. Title = "Unavailable"
    Transparent Label 49. Title = "Unavailable"
.otherwise

.if end


